<mapper namespace="com.ly.shopping.dao.EditionsMapper" >
  <select id="queryedition" resultType="java.lang.String">
    select
   edition
    from xm_edition
    where GoodsID = #{GoodsID,jdbcType=INTEGER}
  </select>
</mapper>